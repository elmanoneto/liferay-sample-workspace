{
	"name": "liferay-sample-module-clay-react",
	"version": "1.0.0",
	"description": "Liferay Sample Module Clay React",
	"devDependencies": {
		"liferay-npm-build-support": "^2.26.0",
		"liferay-npm-bundler": "^2.26.0",
		"liferay-npm-bundler-loader-css-loader": "^2.26.0",
		"copy-webpack-plugin": "4.6.0",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "3.2.1",
		"@babel/cli": "^7.7.5",
		"@babel/core": "^7.7.5",
		"@babel/plugin-transform-runtime": "^7.14.5",
		"@babel/preset-react": "^7.7.4",
		"@babel/preset-env": "^7.7.6"
	},
	"dependencies": {
		"@babel/runtime": "^7.14.6",
		"@clayui/button": "^3.32.0",
		"@clayui/css": "^3.35.3",
		"@clayui/form": "^3.35.3",
		"@clayui/layout": "^3.32.0",
		"@clayui/provider": "^3.35.3",
		"@clayui/table": "^3.32.0",
		"axios": "^0.21.4",
		"react": "16.8.6",
		"react-dom": "16.8.6"
	},
	"scripts": {
		"build": "babel --source-maps -d build src && npm run copy-assets && liferay-npm-bundler",
		"copy-assets": "lnbs-copy-assets",
		"translate": "lnbs-translate",
		"deploy": "npm run build && lnbs-deploy",
		"start": "lnbs-start"
	},
	"portlet": {
		"com.liferay.portlet.display-category": "category.sample",
		"com.liferay.portlet.header-portlet-css": "/css/styles.css",
		"com.liferay.portlet.instanceable": true,
		"javax.portlet.name": "liferaysamplemoduleclayreact",
		"javax.portlet.security-role-ref": "power-user,user",
		"javax.portlet.resource-bundle": "content.Language"
	},
	"main": "index.js"
